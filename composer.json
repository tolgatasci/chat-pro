{
  "name": "tolgatasci/chat",
  "description": "Laravel için gelişmiş ve güvenli bir sohbet paketi",
  "type": "library",
  "keywords": [
    "laravel",
    "realtime-chat",
    "chat",
    "messenger"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Tolga Taşcı",
      "email": "tolgatasci1@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/support": "^8.0|^9.0|^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "orchestra/testbench": "^7.45",
    "laravel/legacy-factories": "^1.1"
  },
  "autoload": {
    "psr-4": {

      "TolgaTasci\\Chat\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TolgaTasci\\Chat\\Tests\\": "tests/",
      "TolgaTasci\\Chat\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TolgaTasci\\Chat\\Providers\\ChatServiceProvider"
      ],
      "aliases": {
        "Chat": "TolgaTasci\\Chat\\Facades\\Chat"
      }
    }
  }
}
